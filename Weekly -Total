// Total Return // 

SELECT currency, 
count(distinct email) as return_customers,
avg(nitems) as item_per_basket,
count(distinct order_number) as orders,
sum(subtotal)/count(distinct order_number) as AOV, 
sum(subtotal) as Revenue
FROM `transactions` 
WHERE postdate between "2017-07-17" and "2017-07-23" 
and email in 
(select email from transactions where postdate<"2017-07-17") 
group by currency 

//Total New//

SELECT currency, 
count(distinct email) as return_customers,
avg(nitems) as item_per_basket,
count(distinct order_number) as orders,
sum(subtotal)/count(distinct order_number) as AOV, 
sum(subtotal) as Revenue
FROM `transactions` 
WHERE postdate between "2016-07-18" and "2016-07-24" 
and email not in 
(select email from transactions where postdate<"2016-07-18") 
group by currency 
