// Total Return // 

SELECT currency, 
count(distinct email) as return_customers,
avg(nitems) as item_per_basket,
count(distinct order_number) as orders,
sum(subtotal)/count(distinct order_number) as AOV, 
sum(subtotal) as Revenue
FROM `transactions` 
WHERE postdate between "2017-11-13" and "2017-11-19" 
and email in 
(select email from transactions where postdate<"2017-11-13") 
group by currency 

//Total New//

SELECT currency, 
count(distinct email) as return_customers,
avg(nitems) as item_per_basket,
count(distinct order_number) as orders,
sum(subtotal)/count(distinct order_number) as AOV, 
sum(subtotal) as Revenue
FROM `transactions` 
WHERE postdate between "2016-11-14" and "2016-11-20" 
and email not in 
(select email from transactions where postdate<"2016-11-14") 
group by currency 
