// Total Return // 

SELECT currency, 
count(distinct email) as return_customers,
avg(nitems) as item_per_basket,
count(distinct order_number) as orders,
sum(subtotal)/count(distinct order_number) as AOV, 
sum(subtotal) as Revenue
FROM `transactions` 
WHERE postdate between "2017-11-06" and "2017-11-12" 
and email in 
(select email from transactions where postdate<"2017-11-06") 
group by currency 

//Total New//

SELECT currency, 
count(distinct email) as return_customers,
avg(nitems) as item_per_basket,
count(distinct order_number) as orders,
sum(subtotal)/count(distinct order_number) as AOV, 
sum(subtotal) as Revenue
FROM `transactions` 
WHERE postdate between "2016-08-29" and "2016-09-04" 
and email not in 
(select email from transactions where postdate<"2016-08-29") 
group by currency 
